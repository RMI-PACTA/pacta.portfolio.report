{
  "private": true,
  "scripts": {
    "license:setup": "printf 'The following are the licenses for the various bundled libraries' > ../LICENSE.note",
    "license:bootstrap": "printf '\n\n# Bootstrap\n' >> ../LICENSE.note && cat node_modules/bootstrap/LICENSE >> ../LICENSE.note",
    "license:bootstrap-select": "printf '\n\n# Bootstrap-select\n' >> ../LICENSE.note && cat node_modules/bootstrap-select/LICENSE >> ../LICENSE.note",
    "license:canvas-toBlob": "printf '\n\n# canvas-toBlob\n' >> ../LICENSE.note && cat node_modules/canvas-toBlob/LICENSE.md >> ../LICENSE.note",
    "license:canvg": "printf '\n\n# canvg\n' >> ../LICENSE.note && cat node_modules/canvg/LICENSE >> ../LICENSE.note",
    "license:d3": "printf '\n\n# D3\n' >> ../LICENSE.note && cat node_modules/d3/LICENSE >> ../LICENSE.note",
    "license:filesaver": "printf '\n\n# FileSaver\n' >> ../LICENSE.note && cat node_modules/file-saver/LICENSE.md >> ../LICENSE.note",
    "license:html2canvas": "printf '\n\n# html2canvas\n' >> ../LICENSE.note && cat node_modules/html2canvas/LICENSE >> ../LICENSE.note",
    "license:jquery": "printf '\n\n# jquery\n' >> ../LICENSE.note && cat node_modules/jquery/LICENSE.txt >> ../LICENSE.note",
    "license:line-awesome": "printf '\n\n# line-awesome\n' >> ../LICENSE.note && cat node_modules/line-awesome/LICENSE.md >> ../LICENSE.note",
    "license:mdbootstrap": "printf '\n\n# Material Design for Bootstrap\n' >> ../LICENSE.note && cat node_modules/mdbootstrap/license.txt >> ../LICENSE.note",
    "license:roboto-fontface": "printf '\n\n# Roboto fontface\n' >> ../LICENSE.note && cat node_modules/roboto-fontface/LICENSE >> ../LICENSE.note",
    "license:tippy": "printf '\n\n# tippy.js\n' >> ../LICENSE.note && cat node_modules/tippy.js/LICENSE >> ../LICENSE.note",
    "license:topojson-client": "printf '\n\n# topojson-client\n' >> ../LICENSE.note && cat node_modules/topojson-client/LICENSE >> ../LICENSE.note",
    "license:world-atlas": "printf '\n\n# world-atlas\n' >> ../LICENSE.note && cat node_modules/world-atlas/LICENSE >> ../LICENSE.note",
    "license": "npm run license:setup && npm run license:bootstrap && npm run license:bootstrap-select && npm run license:canvas-toBlob && npm run license:canvg && npm run license:d3 && npm run license:filesaver && npm run license:html2canvas && npm run license:jquery && npm run license:line-awesome && npm run license:mdbootstrap && npm run license:roboto-fontface && npm run license:tippy && npm run license:topojson-client && npm run license:world-atlas",
    "build:setup": "mkdir -p ../inst/css && mkdir -p ../inst/font/Roboto && mkdir -p ../inst/js && mkdir -p ../inst/webfonts",
    "build:bootstrap:css": "cp node_modules/bootstrap/dist/css/bootstrap.min.css ../inst/css/bootstrap.min.css",
    "build:bootstrap:css.map": "cp node_modules/bootstrap/dist/css/bootstrap.min.css.map ../inst/css/bootstrap.min.css.map",
    "build:bootstrap:js": "cp node_modules/bootstrap/dist/js/bootstrap.min.js ../inst/js/bootstrap.min.js",
    "build:bootstrap:js.map": "cp node_modules/bootstrap/dist/js/bootstrap.min.js.map ../inst/js/bootstrap.min.js.map",
    "build:bootstrap": "npm run build:bootstrap:css && npm run build:bootstrap:css.map && npm run build:bootstrap:js && npm run build:bootstrap:js.map",
    "build:bootstrap-select:css": "cp node_modules/bootstrap-select/dist/css/bootstrap-select.min.css ../inst/css/bootstrap-select.min.css",
    "build:bootstrap-select:css.map": "cp node_modules/bootstrap-select/dist/css/bootstrap-select.css.map ../inst/css/bootstrap-select.css.map",
    "build:bootstrap-select:js": "cp node_modules/bootstrap-select/dist/js/bootstrap-select.min.js ../inst/js/bootstrap-select.min.js",
    "build:bootstrap-select:js.map": "cp node_modules/bootstrap-select/dist/js/bootstrap-select.min.js.map ../inst/js/bootstrap-select.min.js.map",
    "build:bootstrap-select": "npm run build:bootstrap-select:css && npm run build:bootstrap-select:css.map && npm run build:bootstrap-select:js && npm run build:bootstrap-select:js.map",
    "build:canvas-toBlob": "cp node_modules/canvas-toBlob/canvas-toBlob.js ../inst/js/canvas-toBlob.js",
    "build:canvg": "cp node_modules/canvg/dist/browser/canvg.min.js ../inst/js/canvg.min.js",
    "build:d3": "cp node_modules/d3/build/d3.min.js ../inst/js/d3.v4.min.js",
    "build:d3-array": "cp node_modules/d3-array/dist/d3-array.min.js ../inst/js/d3-array.v2.min.js",
    "build:d3-format": "cp node_modules/d3-format/dist/d3-format.min.js ../inst/js/d3-format.v1.min.js",
    "build:d3-geo-projection": "cp node_modules/d3-geo-projection/dist/d3-geo-projection.min.js ../inst/js/d3-geo-projection.v2.min.js",
    "build:d3-scale": "cp node_modules/d3-scale/dist/d3-scale.min.js ../inst/js/d3-scale.v3.min.js",
    "build:d3-scale-chromatic": "cp node_modules/d3-scale-chromatic/dist/d3-scale-chromatic.min.js ../inst/js/d3-scale-chromatic.v1.min.js",
    "build:d3-selection": "cp node_modules/d3-selection/dist/d3-selection.min.js ../inst/js/d3-selection.v1.min.js",
    "build:filesaver": "cp node_modules/file-saver/dist/FileSaver.min.js* ../inst/js/",
    "build:html2canvas": "cp node_modules/html2canvas/dist/html2canvas.min.js ../inst/js/html2canvas.min.js",
    "build:jquery": "cp node_modules/jquery/dist/jquery.min.js ../inst/js/jquery-3.5.1.js",
    "build:line-awesome:fonts": "cp node_modules/line-awesome/dist/font-awesome-line-awesome/webfonts/fa-solid-900* ../inst/webfonts/",
    "build:line-awesome:css": "cp node_modules/line-awesome/dist/font-awesome-line-awesome/css/all.min.css ../inst/css/fontawesome.all.min.css",
    "build:line-awesome": "npm run build:line-awesome:fonts && npm run build:line-awesome:css",
    "build:mdbootstrap:css": "cp node_modules/mdbootstrap/css/mdb.min.css ../inst/css/mdb.min.css",
    "build:mdbootstrap:js": "cp node_modules/mdbootstrap/js/mdb.min.js ../inst/js/mdb.min.js",
    "build:mdbootstrap": "npm run build:mdbootstrap:css && npm run build:mdbootstrap:js",
    "build:popper:js": "cp node_modules/popper.js/dist/umd/popper.min.js ../inst/js/popper.min.js",
    "build:popper:js.map": "cp node_modules/popper.js/dist/umd/popper.min.js.map ../inst/js/popper.min.js.map",
    "build:popper": "npm run build:popper:js && npm run build:popper:js.map",
    "build:roboto-fontface:ttf": "cp node_modules/roboto-fontface/fonts/roboto/*.ttf ../inst/font/Roboto/",
    "build:roboto-fontface": "npm run build:roboto-fontface:ttf",
    "build:tippy:css": "cp node_modules/tippy.js/themes/light.css ../inst/css/tippy_light.css",
    "build:tippy:js": "mkdir -p ../inst/js/tippy.js && cp node_modules/tippy.js/umd/*min* ../inst/js/tippy.js/",
    "build:tippy": "npm run build:tippy:css && npm run build:tippy:js",
    "build:topojson-client": "cp node_modules/topojson-client/dist/topojson-client.min.js ../inst/js/topojson-client.min.js",
    "build:world-atlas": "echo 'const world = '$(cat node_modules/world-atlas/countries-110m.json) > ../inst/js/countries-110m.js",
    "build": "npm run build:setup && npm run build:bootstrap && npm run build:bootstrap-select && npm run build:canvas-toBlob && npm run build:canvg && npm run build:d3 && npm run build:d3-array && npm run build:d3-format && npm run build:d3-geo-projection && npm run build:d3-scale && npm run build:d3-scale-chromatic && npm run build:d3-selection && npm run build:filesaver && npm run build:html2canvas && npm run build:jquery && npm run build:line-awesome && npm run build:mdbootstrap && npm run build:popper && npm run build:roboto-fontface && npm run build:tippy && npm run build:topojson-client && npm run build:world-atlas",
    "all": "npm run license && npm run build"
  },
  "dependencies": {
    "bootstrap": "~4.5.3",
    "bootstrap-select": "~1.13.18",
    "canvas-toBlob": "~1.0.0",
    "canvg": "~1.5.3",
    "d3": "~4.13.0",
    "d3-array": "~2.4.0",
    "d3-format": "~1.4.4",
    "d3-geo-projection": "~2.9.0",
    "d3-scale": "~3.2.1",
    "d3-scale-chromatic": "~1.5.0",
    "d3-selection": "~1.4.1",
    "file-saver": "~2.0.5",
    "html2canvas": "~1.0.0",
    "jquery": "~3.5.1",
    "line-awesome": "github:icons8/line-awesome",
    "mdbootstrap": "~4.8.11",
    "popper.js": "~1.16.1",
    "roboto-fontface": "0.9.0",
    "tippy.js": "~4.3.5",
    "topojson-client": "^3.1.0",
    "world-atlas": "~2.0.2"
  }
}
